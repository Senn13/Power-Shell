{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#8be9fd",
          "style": "plain",
          "template": "{{ .UserName }}@{{ .HostName }}",
          "type": "session"
        },
        {
          "foreground": "#ff79c6",
          "style": "plain",
          "template": " <#f8f8f2>in</> {{ .Path }}",
          "type": "path",
          "properties": {
            "style": "full",
            "folder_separator_icon": "/"
          }
        },
        {
          "foreground": "#f1fa8c",
          "style": "plain",
          "template": " <#f8f8f2>on</> {{ .HEAD }}",
          "type": "git",
          "properties": {
            "branch_icon": ""
          }
        },
        {
          "foreground": "#50fa7b",
          "style": "plain",
          "template": " $",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
