{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#6272a4",
          "style": "powerline",
          "template": " {{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#8be9fd",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path",
          "properties": {
            "style": "full",
            "folder_separator_icon": "/"
          }
        },
        {
          "background": "#f1fa8c",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ .HEAD }} ",
          "type": "git",
          "properties": {
            "branch_icon": ""
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
